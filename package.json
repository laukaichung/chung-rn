{
  "name": "chung-rn",
  "version": "0.2.53",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "react native",
    "ui"
  ],
  "description": "A React Native UI framework based on ant-design-mobile-rn",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stonecold123/chung-rn.git"
  },
  "author": "Lau Kai Chung",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stonecold123/chung-rn/issues"
  },
  "homepage": "https://github.com/stonecold123/chung-rn#readme",
  "scripts": {
    "androidBuild": "cd ./android && ./gradlew app:assembleRelease && ./gradlew installRelease",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "TS_JEST_DEBUG=true jest",
    "menu": "adb shell input keyevent 82",
    "compileDebug": "cd ./android && ./gradlew assembleDebug --info",
    "clean": "cd ./android && ./gradlew clean",
    "compile": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && react-native run-android",
    "android": "react-native run-android",
    "port": "lsof -t -i:8081",
    "debugSetting": "adb reverse tcp:9090 tcp:9090",
    "backup": "tar -cvf components.tar ./components"
  },
  "dependencies": {
    "react-native-modal": "^6.5.0",
    "react-native-modal-datetime-picker": "^6.0.0",
    "rmc-tabs": "^1.2.27",
    "rn-topview": "^0.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.116",
    "@types/moment": "^2.13.0",
    "@types/node": "^10.9.4",
    "@types/react": "^16.4.18",
    "@types/react-native": "0.57.7",
    "@types/react-test-renderer": "^16.0.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "babel-plugin-graphql-tag": "^1.6.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-react-native": "^5.0.2",
    "babel-runtime": "^7.0.0-beta.3",
    "identity-obj-proxy": "^3.0.0",
    "ignore-styles": "^5.0.1",
    "install": "^0.12.2",
    "isomorphic-fetch": "^2.2.1",
    "jest": "23.6.0",
    "jsdom": "11.12.0",
    "jsdom-global": "3.0.2",
    "metro-react-native-babel-preset": "0.48.1",
    "npm": "^6.4.1",
    "react": "^16.6.0",
    "react-dom": "^16.4.2",
    "react-native": "^0.57.4",
    "react-navigation": "^2.18.0",
    "react-test-renderer": "16.6.0-alpha.8af6728",
    "reactotron-react-native": "^2.0.0",
    "ts-jest": "^23.1.3",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.1"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "^.+\\.tsx?$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.jest.json"
      },
      "window": {}
    },
    "testRegex": "(/chung_rn/__tests__/.*|(\\.|/)(test|spec))\\.(jsxxxx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/FileMock.js"
    }
  }
}
